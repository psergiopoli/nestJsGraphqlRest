{
  "name": "graphql-nest",
  "version": "0.0.0",
  "description": "graphql example for learn",
  "author": "psergiopoli",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:prod": "rimraf dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development nodemon",
    "start:debug": "NODE_ENV=debug nodemon --config nodemon-debug.json",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm -f cli-ormconfig.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/graphql": "^6.0.5",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/typeorm": "^6.0.0",
    "apollo-server-express": "^2.4.8",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "express-http-context": "^1.2.1",
    "graphql": "^14.2.1",
    "graphql-tools": "^4.0.4",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.1",
    "typeorm": "^0.2.16",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.7",
    "@types/supertest": "^2.0.7",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.3",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tscpaths": "^0.0.7",
    "tslint": "5.16.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
